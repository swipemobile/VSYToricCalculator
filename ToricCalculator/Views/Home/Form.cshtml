		function senkronX(deg) {
			document.getElementById("axisY").oninput = "";
			if (deg.value == "") document.getElementById("image5").style.display = "none";
			else {
				if (deg.value > 180) {
					document.getElementById("axisX").style.backgroundColor = "#FFF0F0";
					document.getElementById("axisX").style.color = "#ff2d2d";
					/*document.getElementById("validmessage").style.display = "";*/
				}
				else {
					document.getElementById("image5").style.transform = ("rotate(" + -deg.value + "deg)");
					document.getElementById("image5").style.display = "";
					/*document.getElementById("validmessage").style.display = "none";*/
					document.getElementById("axisX").style.backgroundColor = "";
					document.getElementById("axisX").style.color = "";
				}
			}

			if (deg.value != null && deg.value != "") {
				if (deg.value < 91) {
					document.getElementById("axisY").value = Math.abs(parseInt(deg.value) + 90);
					document.getElementById("image6").style.transform = ("rotate(" + Math.abs(-parseInt(deg.value) + 90) + "deg)");
				}
				else {
					document.getElementById("axisY").value = Math.abs(deg.value - 90);
					document.getElementById("image6").style.transform = ("rotate(" + Math.abs(-deg.value) + "deg)");
				}
			}
			else document.getElementById("axisY").value = "";
			if (deg.value == "") {
				document.getElementById("image6").style.display = "none";
			}
			else document.getElementById("image6").style.display = "";
			if (document.getElementById("axisY").value < 181) {
				document.getElementById("axisY").style.backgroundColor = "";
				document.getElementById("axisY").style.color = "";
			}
			else {
				document.getElementById("axisY").style.backgroundColor = "#FFF0F0";
				document.getElementById("axisY").style.color = "#ff2d2d";
			}
			document.getElementById("axisY").oninput = function () { senkronY(this); };
			if (deg.value == "" || deg.value == null) {
				document.getElementById("axisY").style.backgroundColor = "";
				document.getElementById("axisY").style.color = "";
				document.getElementById("axisX").style.backgroundColor = "";
				document.getElementById("axisX").style.color = "";
			}
		}
